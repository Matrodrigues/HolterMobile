        function geraGraficoBatimento(data) {
            var chartColours = ['#96CA59', '#3F97EB', '#72c380', '#6f7a8a', '#f7cb38', '#5a8022', '#2c7282'];

            var d1 = [];

            for (var i = 0; i < data.listaMedidas.length; i++) {
                //alert(data.listaMedidas[i].horario.substring(6).replace(')/', '') + '    ' + data.listaMedidas[i].bpm);
                d1.push([data.listaMedidas[i].horario.substring(6).replace(')/', ''), data.listaMedidas[i].bpm]);
            }

            var chartMinDate = d1[0][0]; //first day
            var chartMaxDate = d1[data.listaMedidas.length - 1][0]; //last day

            //graph options
            var options = {

                grid: {
                    show: true,
                    aboveData: true,
                    color: "#3f3f3f",
                    labelMargin: 10,
                    axisMargin: 0,
                    borderWidth: 0,
                    borderColor: null,
                    minBorderMargin: 5,
                    clickable: true,
                    hoverable: true,
                    autoHighlight: true,
                    mouseActiveRadius: 100
                },
                series: {
                    lines: {
                        show: true,
                        fill: true,
                        lineWidth: 2,
                        steps: false
                    },
                    points: {
                        show: true,
                        radius: 4.5,
                        symbol: "circle",
                        lineWidth: 3.0
                    }
                },
                legend: {
                    position: "ne",
                    margin: [0, -25],
                    noColumns: 0,
                    labelBoxBorderColor: null,
                    labelFormatter: function (label, series) {
                        // just add some space to labes
                        return label + '&nbsp;&nbsp;';
                    },
                    width: 40,
                    height: 1
                },
                colors: chartColours,
                timezone: "browser",
                shadowSize: 0,
                tooltip: true, //activate tooltip
                tooltipOpts: {
                    content: "%s: %y.0",
                    xDateFormat: "%d/%m",
                    shifts: {
                        x: -30,
                        y: -50
                    },
                    defaultTheme: false
                },
                yaxis: {
                    min: 0,
                    max: 200,
                    ticks: 10
                },
                xaxis: {
                    mode: "time",
                    minTickSize: [1, "day"],
                    timeformat: "%d/%m/%y",
                    min: chartMinDate,
                    max: chartMaxDate
                }
            };

            var plot = $.plot($("#placeholder33x"), [{
                label: "Batimento Cardíaco",
                data: d1,
                lines: {
                    fillColor: "rgba(150, 202, 89, 0.12)"
                }, //#96CA59 rgba(150, 202, 89, 0.42)
                points: {
                    fillColor: "#fff"
                }
            }], options);
        }

